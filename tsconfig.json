{
	"compilerOptions": {
		"checkJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"paths": {
			"react": ["./node_modules/preact/compat/"],
			"react-dom": ["./node_modules/preact/compat/"]
		},
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"verbatimModuleSyntax": true
	}
}
